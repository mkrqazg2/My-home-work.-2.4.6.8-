// Даны два числа. Показать большее и меньшее число (Two numbers are given. Show larger and smaller numbers)
int numberA = 4;
int numberB = 5;
int max;
int min;
 if (numberA > numberB)
 {
 max = numberA;
 min = numberB;
 }
  else
  {
  max = numberB;
  min = numberA;
  }
{
Console.WriteLine("max = " + max);
Console.WriteLine("min =" + min);
}
